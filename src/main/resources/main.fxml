      <Label fx:id="remind" layoutX="832.0" layoutY="489.0" prefHeight="27.0" prefWidth="76.0" text="enter换行" />
      <Label fx:id="remind2" layoutX="832.0" layoutY="573.0" prefHeight="15.0" prefWidth="76.0" text="ctrl+enter" />
      <Label fx:id="messageNum" layoutX="44.0" layoutY="41.0" prefHeight="23.0" prefWidth="2.0" text="" />
      <Button layoutX="789.0" layoutY="459.0" mnemonicParsing="false" onAction="#messageList" prefHeight="27.0" prefWidth="70.0" text="消息记录" />
   </children>
</AnchorPane>
