<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="610.0" prefWidth="921.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.easychat.fx.controller.Main">
   <children>
      <ListView fx:id="userListView" layoutX="5.0" layoutY="40.0" onMouseClicked="#listViewClicked" prefHeight="544.0" prefWidth="178.0" />
      <ListView fx:id="textArea" layoutX="194.0" layoutY="488.0" prefHeight="113.0" prefWidth="630.0"/>
      <Button fx:id="sendMessage" layoutX="832.0" layoutY="516.0" mnemonicParsing="false" onAction="#sendMessage" onKeyReleased="#enter" prefHeight="58.0" prefWidth="76.0" text="发送" />
      <ListView fx:id="messageListView" layoutX="194.0" layoutY="31.0" onMouseClicked="#messagViewClicked" prefHeight="428.0" prefWidth="713.0" />
